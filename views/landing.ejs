<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Home</title>
  <link rel="icon" type="image/x-icon" href="/images/2.png">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <link rel="stylesheet" href="/css/landing.css">
</head>

<body>
  <!-- navbar code -->
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark sticky-top ">
    <div class="container-fluid px-md-5">
      <a class="navbar-brand" href="/">Rent-It</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav"
        aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse justify-content-end " id="navbarNav">
        <ul class="navbar-nav">
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="/landing">Home</a>
          </li>
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="#">About</a>
          </li>
          <li class="nav-item">
            <a class="nav-link active" aria-current="page" href="#">Contact</a>
          </li>
        </ul>
      </div>
    </div>
  </nav>
  <!--  navbar ending -->
  <div class="container">
    <div class="row p-3">
      <div class="col d-flex justify-content-center  ">
        Select the Engine Type of Bike
      </div>
    </div>
    <div class="row">
      <div class="col d-flex justify-content-center ">
        <form action="/page2" method="post">
          <label for="fueltype" class="form-label">Choose the Type: </label>

          <select name="fueltype" id="fueltype" class="form-select ">
            
            <option value="*">All bikes</option>
            <% Object.keys(ftypes).forEach(function(key){ %>
            <% var row = ftypes[key]; %>
            <option value="<%= row.enginetype %>"><%= row.enginetype %> </option>
            <% }) %>
          </select>
          <div class="row ">
            <div class="col my-2 text-center">
              <h3>Available Numbers</h3> 
              <h5 class = "noe">
                
                <% Object.keys(numberofFuels).forEach(function(key){ %>
                  <% var row =numberofFuels[key]; %>
                  <div id="ename<%= row.enginetype %>">
                    <%= row.enginetype %>  : <%= row.count %>
                  </div>
                  <% }) %>

              </h5>
              
            </div>
          </div>
          <div class="row">
            <input class="btn btn-dark my-3" type='submit' />
          </div>
          

        </form>
      </div>
    </div>
  </div>

  
  <!-- JavaScript Bundle with Popper -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
    integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous">
  </script>
  <script src="/js/landing1.js"></script>
</body>

</html>